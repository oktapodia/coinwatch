    "style-loader": "^0.19.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@exponent/electron-cookies": "^2.0.0",
    "auto-launch": "^5.0.1",
    "axios": "^0.17.0",
    "bootstrap": "^3.3.7",
    "devtron": "^1.4.0",
    "electron-debug": "^1.4.0",
    "electron-log": "^2.2.11",
    "electron-osx-sign": "^0.4.7",
    "electron-settings": "^3.1.3",
    "electron-updater": "^2.21.10",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "menubar": "^5.2.3",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-addons-shallow-compare": "^15.6.2",
